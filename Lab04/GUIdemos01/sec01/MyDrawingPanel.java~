 */
public class MyDrawingPanel extends JPanel
{
   // properties

   // constructors
   public MyDrawingPanel()
   {
      
       setSize( 300, 250);   
       setBackground(Color.yellow);
       getContentPane().setBackground(Color.yellow);
      
   }
   
   // methods

}